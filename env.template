OPENAI_API_KEY=sk-proj-Du-your_openai_api_key_here
NEWSAPI_KEY=your_newsapi_key_here

# Semantic Cache Configuration (optional)
# Enable semantic caching to save API costs by reusing similar responses
SEMANTIC_CACHE_ENABLED=true
SEMANTIC_CACHE_THRESHOLD=0.85
SEMANTIC_CACHE_SIZE=1000
SEMANTIC_CACHE_TTL=3600
# Optional: Custom cache file path (default: semantic_cache.json in project root)
# SEMANTIC_CACHE_FILE=semantic_cache.json

# LangSmith Tracing Configuration (optional)
# Enable LangSmith for workflow tracing and observability
# Get your API key from https://smith.langchain.com/
LANGSMITH_ENABLED=false
LANGSMITH_API_KEY=your_langsmith_api_key_here
# Optional: Project name for organizing traces (default: uses default project)
# LANGSMITH_PROJECT=crypto-analysis-agent